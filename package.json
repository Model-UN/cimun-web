    "axios": "^1.4.0",
    "md5": "^2.3.0",
    "next": "^13.4.6",
    "nextjs-redirect": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3",
    "vercel": "^23.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-actions": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
